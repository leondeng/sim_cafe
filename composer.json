{
    "require": {
        "phpunit/phpunit": "5.5.*"
    },
    "autoload": {
        "psr-4": {
            "Simcafe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simcafe\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "php vendor/bin/phpunit"
        ],
        "test:coverage": [
            "php vendor/bin/phpunit --coverage-html tests/report"
        ]
    }

}
